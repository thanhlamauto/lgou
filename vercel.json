{
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "functions": {
    "api/upload.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "rewrites": [
    {
      "source": "/cms",
      "destination": "/cms.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
